@model POS.ViewModel.CategoryModel

@{
    ViewData["Title"] = "Category Detail";
}

<h2>Category Detail</h2>
<table class="table tab-content">
    <tbody>
        <tr>
            <th>ID</th>
            <td>@Model.Id</td>
        </tr>
        <tr>
            <th>Category Name</th>
            <td>@Model.CategoryName</td>
        </tr>
        <tr>
            <th>Description</th>
            <td>@Model.Description</td>
        </tr>
    </tbody>
</table>
